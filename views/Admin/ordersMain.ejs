                        <td class="whitespace-nowrap px-6 py-4">COD</td>
                        <td class="whitespace-nowrap px-6 py-4">169.00</td>
                      </tr>
                      <tr
                        class="border-b transition duration-300 ease-in-out hover:bg-neutral-100 dark:border-neutral-500 dark:hover:bg-neutral-600">
                        <td class="whitespace-nowrap px-6 py-4 font-medium">ORD202310131116169780</td>
                        <td class="whitespace-nowrap px-6 py-4">13/02/2002</td>
                        <td class="whitespace-nowrap px-6 py-4">Delbin George</td>
                        <td class="whitespace-nowrap px-6 py-4">Saffron Almond Milk</td>
                        <td class="whitespace-nowrap px-6 py-4">Saffron Almond Milk</td>
                        <td class="whitespace-nowrap px-6 py-4">Saffron Almond Milk</td>
                        <td class="whitespace-nowrap px-6 py-4">Saffron Almond Milk</td>
                      </tr>
                      <tr
                        class="border-b transition duration-300 ease-in-out hover:bg-neutral-100 dark:border-neutral-500 dark:hover:bg-neutral-600">
                        <td class="whitespace-nowrap px-6 py-4 font-medium">ORD202310131116169780</td>
                        <td class="whitespace-nowrap px-6 py-4">13/02/2002</td>
                        <td class="whitespace-nowrap px-6 py-4">Alfin Saji</td>
                        <td class="whitespace-nowrap px-6 py-4">M Crunchy Burger</td>
                        <td class="whitespace-nowrap px-6 py-4">M Crunchy Burger</td>
                        <td class="whitespace-nowrap px-6 py-4">M Crunchy Burger</td>
                        <td class="whitespace-nowrap px-6 py-4">M Crunchy Burger</td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
    </section>
        <!-- Footer -->
        <%- include('adminFooter') %>
</body>

</html>